{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Aspire": {
    "StackExchange": {
      "Redis": {
        "ConfigurationOptions": {
          "ConnectTimeout": 900000,
          "ConnectRetry": 2
        },
        "HealthChecks": true,
        "Tracing": true,
        "ConnectionString": "localhost:6379"
      }
    }
  }
}
